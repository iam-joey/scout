{
  "name": "express-ts",
  "module": "src/app.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "bun run src/app.ts",
    "dev": "bun --watch src/app.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.{ts,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,json}\""
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "prettier": "^3.5.3"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@solana/web3.js": "^1.98.0",
    "@types/express": "^5.0.1",
    "@types/ws": "^8.18.1",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "form-data": "^4.0.2",
    "redis": "^4.7.0",
    "ws": "^8.18.1",
    "zod": "^3.24.2"
  }
}